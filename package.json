    "loopback": "^2.29.1",
    "loopback-datasource-juggler": "^2.47.0",
    "mocha": "^2.5.3",
    "mocha-given": "^0.1.3",
    "snazzy": "^4.0.0",
    "standard": "^7.1.2",
    "testem": "^1.10.0-1",
    "xyz": "^0.5.0"
