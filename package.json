    "node": ">=6.0.0"
  },
  "scripts": {
    "lint": "standard --verbose | snazzy",
    "pretest": "npm run lint",
    "test:dev": "ava --watch",
    "test": "ava --verbose",
    "build": "babel src --presets babel-preset-es2015 --out-dir dist",
    "prepublish": "npm test && npm prune && npm run build",
